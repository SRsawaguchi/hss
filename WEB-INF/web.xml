	<!-- TestReset -->
	<servlet>
		<servlet-name>testreset</servlet-name>
		<servlet-class>jp.co.u_sha.hssystem.TestReset</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>testreset</servlet-name>
		<url-pattern>/testreset</url-pattern>
	</servlet-mapping>

</web-app>