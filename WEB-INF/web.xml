	</servlet>
	<servlet-mapping>
		<servlet-name>wakuclick</servlet-name>
		<url-pattern>/wakuclick</url-pattern>
	</servlet-mapping>
	
	<!-- KakuteiClick -->
	<servlet>
		<servlet-name>kakuteiclick</servlet-name>
